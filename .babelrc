// 바벨은 preset정의 (바벨 역할을 모아놓음)
{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "src": "src"
                }
            }
        ]
    ]
}